<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Графические файлы</title>
  <style>
    img {
      width: 200px;
      height: auto;
      margin: 10px;
    }
  </style>
</head>
<body>
  <h1>Графические файлы</h1>
  <div id="imagesContainer"></div>

  <script>
    // Пусть picsArray содержит имена всех файлов (pic1.gif, pic2.gif, и т.д.)
    const picsArray = ["wai.gif", "SeWo.gif", "tenor.gif", "g4xfc.gif", "dance-anime.gif", "1567341836_hot2.gif"]; // добавьте остальные файлы в массив

    const imagesContainer = document.getElementById("imagesContainer");

    picsArray.forEach(pic => {
      const img = document.createElement("img");
      img.src = pic;
      imagesContainer.appendChild(img);
    });
  </script>
</body>
</html>
